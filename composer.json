{
    "name": "underdog/reports-rosters",
    "description": "Generates event rosters and reports for InterSoccer Switzerland admins using WooCommerce data.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "phpoffice/phpspreadsheet": "^1.29",
        "maennchen/zipstream-php": "^2.1 || ^3.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "yoast/phpunit-polyfills": "^1.0",
        "mockery/mockery": "^1.5",
        "brain/monkey": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "InterSoccer\\ReportsRosters\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InterSoccer\\ReportsRosters\\Tests\\": "tests/"
        }
    },
    "license": "Apache2",
    "authors": [
        {
            "name": "Jeremy Lee",
            "email": "jlee@underdogunlimited.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "8.2.0"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "test:unit": "phpunit --testsuite=Unit",
        "test:integration": "phpunit --testsuite=Integration"
    }
}
